{
  "name": "backend-template-express",
  "version": "1.0.0",
  "main": "dist/index.js",
  "description": "A production ready boilerplate for building REST APIs using Node.js, Express, Prisma, and TypeScript.",
  "license": "MIT",
  "author": {
    "name": "Nadeem Ashraf",
    "email": "dev.nadeemashraf06@gmail.com",
    "url": "https://github.com/devnadeemashraf"
  },
  "keywords": [
    "node",
    "jest",
    "prisma",
    "routes",
    "usecases",
    "repositories",
    "middlewares"
  ],
  "scripts": {
    "start": "node -r tsconfig-paths/register dist/index.js",
    "build": "tsc && tsc-alias",
    "dev": "ts-node-dev -r tsconfig-paths/register --transpile-only --respawn --ignore-watch node_modules src/index.ts",
    "test": "jest",
    "test:dev": "jest --watch",
    "format": "prettier --write .",
    "lint": "eslint .",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "prisma:format": "yarn prisma:format:app && yarn prisma:format:logs",
    "prisma:format:app": "npx prisma format --schema=./prisma/app.schema.prisma",
    "prisma:format:logs": "npx prisma format --schema=./prisma/logs.schema.prisma",
    "prisma:generate": "yarn prisma:generate:app && yarn prisma:generate:logs",
    "prisma:generate:app": "npx prisma generate --schema=./prisma/app.schema.prisma",
    "prisma:generate:logs": "npx prisma generate --schema=./prisma/logs.schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.8.1",
    "arrpag": "^2.1.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pino": "^9.6.0",
    "redis": "^5.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@eslint/json": "^0.12.0",
    "@swc/core": "^1.3.105",
    "@swc/jest": "^0.2.31",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "eslint": "^9.26.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "3.5.3",
    "prisma": "^5.8.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.31.1"
  }
}
